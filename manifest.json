{

  "manifest_version": 2,
  "name": "GDPR Not Consent",
  "version": "1.2",

  "description": "Hides GDPR Consent Dialogs",

  "applications": {
		"gecko": {
			"id": "gdpr-not-concent@csicar.de"
		}
  },
  "permissions": [
		"webRequest",
		"webRequestBlocking",
		"*://*/*"
	],
	"background": {
		"scripts": [
			"blocked_domains.js",
			"background.js"
		]
	},
	"content_scripts": [
		{
			"matches": [
				"*://*/*"
			],
			"js": [
				"content-script.js"
			]
		}
	]

}